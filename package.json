{
  "name": "zenika-a11y-components",
  "version": "0.0.0",
  "description": "The Zenika A11Y Component project provide a very basic and versatile design toolkit to design accessible components.",
  "type": "module",
  "scripts": {
    "test": "npm run lint && npm run e2e",
    "e2e": "cucumber-js tests/features --esm --require tests/support/**/*.js -f html:tests/report.html -f progress-bar --format-options '{ \"snippetInterface\": \"async-await\" }' --publish-quiet",
    "lint": "eslint html/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenika-open-source/zenika-a11y-components.git"
  },
  "keywords": [
    "a11y"
  ],
  "author": "Zenika",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/zenika-open-source/zenika-a11y-components/issues"
  },
  "homepage": "https://github.com/zenika-open-source/zenika-a11y-components#readme",
  "devDependencies": {
    "@cucumber/cucumber": "7.2.0",
    "eslint": "^7.32.0",
    "local-web-server": "^5.1.0",
    "playwright": "^1.14.0"
  }
}
